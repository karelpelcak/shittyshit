{"code":"import { useMemo } from 'react';\r\nimport { useContextSelector } from 'use-context-selector';\r\nimport { BookingContext } from '../store/booking/reducer';\r\nconst useBookingPrice = () => {\r\n    const thereAddons = useContextSelector(BookingContext, (c) => c.state.booking?.there?.selectedAddons);\r\n    const backAddons = useContextSelector(BookingContext, (c) => c.state.booking?.back?.selectedAddons);\r\n    const therePrice = useContextSelector(BookingContext, (c) => c.state.booking?.there?.price);\r\n    const backPrice = useContextSelector(BookingContext, (c) => c.state.booking?.back?.price);\r\n    const backCarbonOffsetPrice = useContextSelector(BookingContext, (c) => c.state.booking?.there?.carbonOffset?.amount);\r\n    const thereCarbonOffsetPrice = useContextSelector(BookingContext, (c) => c.state.booking?.there?.carbonOffset?.amount);\r\n    //eslint-disable-next-line no-console\r\n    console.log('ðŸš€ ~ useBookingPrice ~ backCarbonOffsetPrice:', backCarbonOffsetPrice);\r\n    //eslint-disable-next-line no-console\r\n    console.log('ðŸš€ ~ useBookingPrice ~ thereCarbonOffsetPrice:', thereCarbonOffsetPrice);\r\n    const addonsPrice = useMemo(() => [...(thereAddons || []), ...(backAddons || [])].reduce((prev, curr) => prev + (curr.price * curr.count), 0), [thereAddons, backAddons]);\r\n    const bookingPrice = (therePrice ?? 200) + (backPrice ?? 200);\r\n    const CarbonOffsetPrice = (backCarbonOffsetPrice ?? 0) + (thereCarbonOffsetPrice ?? 0);\r\n    const totalPrice = bookingPrice + addonsPrice + CarbonOffsetPrice;\r\n    return { addonsPrice, backPrice, bookingPrice, therePrice, totalPrice, CarbonOffsetPrice };\r\n};\r\nexport default useBookingPrice;\r\n//# sourceMappingURL=useBookingPrice.js.map","references":["/home/karelpelcak/Documents/repo/rj-shop-bl/node_modules/@types/react/index.d.ts","/home/karelpelcak/Documents/repo/rj-shop-bl/node_modules/use-context-selector/dist/src/index.d.ts","/home/karelpelcak/Documents/repo/rj-shop-bl/src